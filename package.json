{
  "name": "cocono",
  "version": "0.1.0",
  "bin": {
    "cocono": "bin/cocono.js"
  },
  "scripts": {
    "build:app": "parcel build src/main/index.ts src/notify/index.ts --target=node --bundle-node-modules",
    "watch:app": "parcel watch src/main/index.ts src/notify/index.ts --target=node --bundle-node-modules",
    "build:cdk": "tsc",
    "watch:cdk": "tsc -w",
    "build:docs": "parcel build src/docs/index.ts --target=node --bundle-node-modules --out-dir=dist/docs && cp -r docs/ dist/docs",
    "serve:docs": "serve ./docs/",
    "cdk": "cdk",
    "test": "jest"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.11",
    "@types/lodash": "^4.14.119",
    "aws-cdk": "^0.21.0",
    "jest": "^23.6.0",
    "parcel-bundler": "^1.11.0",
    "serve": "^10.1.1",
    "ts-jest": "^23.10.5",
    "typescript": "^3.1.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^0.21.0",
    "@aws-cdk/aws-cloudwatch": "^0.21.0",
    "@aws-cdk/aws-lambda": "^0.21.0",
    "@aws-cdk/aws-sns": "^0.21.0",
    "@aws-cdk/cdk": "^0.21.0",
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "serverless-http": "^1.8.0"
  }
}